{
  "name": "cdslogviewer",
  "version": "1.0.0",
  "description": "Frontend for viewing CDS logs",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "clean": "rm -rf node_modules ../public/javascripts/bundle.js",
    "snyk-protect": "snyk protect",
    "lint": "prettier --check app __tests__",
    "lint:fix": "prettier --write app __tests__"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "tsx",
      "ts",
      "json"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest",
      "node_modules/react-multistep/.+\\.js$": "ts-jest"
    },
    "testResultsProcessor": "jest-junit",
    "setupFiles": [
      "./jestSetup.jsx"
    ]
  },
  "dependencies": {
    "@guardian/pluto-headers": "v2.0.4",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "axios": "^1.6.3",
    "buffer": "^6.0.3",
    "can-ndjson-stream": "^1.0.2",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.8.1",
    "date-fns": "^2.21.3",
    "jose": "^4.9.3",
    "jsonwebtoken": "^9.0.1",
    "loader-utils": "2.0.4",
    "process": "^0.11.10",
    "query-string": "6.13.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^11.0.1",
    "semver": "7.5.2",
    "sinon": "^10.0.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "3.3.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "typescript": "^4.2.4",
    "util": "^0.12.3",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.6.0"
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^26.0.23",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "enzyme": "^3.11.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-junit": "^12.0.0",
    "moxios": "^0.4.0",
    "prettier": "^2.2.1"
  },
  "resolutions": {
    "decode-uri-component": "0.2.2"
  }
